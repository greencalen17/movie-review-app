{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@react-native/metro-config": [
        "./node_modules/@react-native/metro-config"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
